{
  "client": {
    "name": "Yowsup2-Node",
    "version": "2.23.24.84",
    "deviceId": "YOWSUP2-",
    "logLevel": "info"
  },
  "transport": {
    "endpoints": [
      { "host": "e1.whatsapp.net", "port": 443 },
      { "host": "e2.whatsapp.net", "port": 443 },
      { "host": "e3.whatsapp.net", "port": 443 },
      { "host": "e4.whatsapp.net", "port": 443 },
      { "host": "e5.whatsapp.net", "port": 443 }
    ],
    "timeout": 30000,
    "reconnectAttempts": 5,
    "reconnectDelay": 1000,
    "heartbeatInterval": 30000
  },
  "crypto": {
    "algorithm": "curve25519",
    "encryption": "aes-256-cbc",
    "mac": "hmac-sha256",
    "preKeyCount": 100
  },
  "sync": {
    "interval": 30000,
    "timeout": 30000,
    "maxRetries": 3
  },
  "media": {
    "maxFileSize": 16777216,
    "supportedTypes": ["image", "video", "audio", "document", "sticker"],
    "imageFormats": ["jpeg", "png", "gif", "webp"],
    "videoFormats": ["mp4", "webm", "mpeg"],
    "audioFormats": ["ogg", "mp3", "wav"]
  },
  "logging": {
    "level": "info",
    "file": "logs/yowsup2.log",
    "maxSize": 10485760,
    "maxFiles": 5
  }
}